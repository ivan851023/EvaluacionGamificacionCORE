<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewBag.Title - Gamificación Prototipo</title>

    <link rel="stylesheet" href="~/kendoui.for.jquery.2021.2.616.commercial/styles/kendo.common.min.css" />
    <link rel="stylesheet" href="~/kendoui.for.jquery.2021.2.616.commercial/styles/kendo.black.min.css" />
    <link rel="stylesheet" href="~/kendoui.for.jquery.2021.2.616.commercial/styles/kendo.default.mobile.min.css" />


    <script src="~/assets/js/jquery-3.1.1.min.js"></script>
    <script src="~/js/jquery.easy-pie-chart.js"></script>
    <script src="~/kendoui.for.jquery.2021.2.616.commercial/js/kendo.all.min.js"></script>
    <script src="~/kendoui.for.jquery.2021.2.616.commercial/js/kendo.grid.min.js"></script>
    <script src="~/kendoui.for.jquery.2021.2.616.commercial/js/jszip.min.js"></script>



    <link href="~/assets/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/assets/css/simple-line-icons.min.css" rel="stylesheet">


    <environment include="Development">
        <link rel="stylesheet" href="~/assets/css/style.css">
        <link rel="stylesheet" href="~/assets/css/site.css" />
    </environment>

    <environment exclude="Development">
        <link rel="stylesheet" href="~/assets/css/style.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/assets/css/site.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/assets/css/bootstrap.min.css" />



    </environment>

    <!-- Styles required by this view -->
    @RenderSection("Styles", required: false)


</head>
<body class="app header-fixed sidebar-fixed aside-menu-fixed aside-menu-hidden">


    @Html.Partial("_AppHeader")


    <div class="app-body">
        <div class="sidebar">

            <!-- *SIDEBAR-NAV* -->
            @Html.Partial("_SidebarNav")
            <!-- /*SIDEBAR-NAV* -->

            <button class="sidebar-minimizer brand-minimizer" type="button"></button>
        </div>

        <!-- *MAIN CONTENT* -->
        <main class="main">

            <!-- *BREADCRUMB* -->
            @Html.Partial("_BreadCrumb")
            <!-- /*BREADCRUMB* -->
            <!-- *CONTAINER-FLUID* -->
            <div class="container-fluid">

                <!-- *PAGE* -->
                @RenderBody()
                <!-- /*PAGE* -->

            </div>
            <!-- /*CONTAINER-FLUID* -->

        </main>
        <!-- /*MAIN CONTENT* -->
        <!-- *ASIDE-MENU* -->
        @Html.Partial("_AsideMenu")
        <!-- /*ASIDE-MENU* -->

    </div>


    <footer class="app-footer">
        <span><a href="#">Ivan</a> © 2021 IvanLabs.</span>
        <span class="ml-auto">Powered by <a href="#">Ivan</a></span>
    </footer>

    <script src="~/assets/js/popper.min.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/plugins/pace/pace.min.js"></script>

    <script src="~/assets/js/plugins/chartJs/Chart.min.js"></script>
    


    <script src="~/js/app.js"></script>

</body>
</html>
